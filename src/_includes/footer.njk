<footer class="container text-gray-600 mb-6 mt-8 sm:mt-16 text-sm flex flex-wrap justify-center items-center">
  <span
    x-data=""
    x-text="`Waler Media LLC | Â© ${new Date().getFullYear()}`"
  ></span>
  <div class="w-full flex items-center justify-center space-x-3 mt-4 md:w-auto md:mt-0 md:ml-6">
    {% for item in navigation.external  %}
      <a
        aria-label="{{ item.icon }} profile link"
        href="{{ item.link }}"
        target="_blank"
        rel="noopener"
        class="w-5 h-5 text-gray-500"
      >
        {% include 'svg/' + item.icon + '.svg' %}
      </a>
    {% endfor  %}
  </div>
</footer>
